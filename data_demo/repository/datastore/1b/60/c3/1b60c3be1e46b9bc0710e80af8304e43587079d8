        <valueClassName>java.lang.Integer</valueClassName>
        <source>select distinct t.employeeid, e.name from timesheet t, employee e&#xd;
 where t.employeeid = e.employeeid &#xd;
 and t.projectid in ${Project}&#xd;
 </source>
